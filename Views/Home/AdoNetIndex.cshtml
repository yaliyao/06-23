@using test_0623_A.Models
@model test_0623_A.Models.DirectoryDataSet.employeeDataTable


<ul data-role="listview" data-filter="true">

    @{foreach (DirectoryDataSet.employeeRow emp in Model) {
            <li>
                <a href="employeeDetails.html?id=1">
                    <img src="~/images/@emp["picture"]">
                    <h4>@emp["firstName"] @emp["lastName"]</h4>
                    <p>@emp["title"]</p> <span class="ui-li-count">4</span>
                </a>
            </li>
        }
    }
</ul>

